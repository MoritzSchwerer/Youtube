class _Fade(Transform):
    point_target: bool
    shift_vector: Incomplete
    scale_factor: Incomplete
    def __init__(self, *mobjects: Mobject, shift: np.ndarray | None = ..., target_position: np.ndarray | Mobject | None = ..., scale: float = ..., **kwargs) -> None: ...

class FadeIn(_Fade):
    def __init__(self, *mobjects: Mobject, **kwargs) -> None: ...
    def create_target(self): ...
    def create_starting_mobject(self): ...

class FadeOut(_Fade):
    def __init__(self, *mobjects: Mobject, **kwargs) -> None: ...
    def create_target(self): ...
    def clean_up_from_scene(self, scene: Scene = ...) -> None: ...
